   "source": [
    "#overplot regression line\n",
    "# x-Achse erstellen von 20-80\n",
    "x_new = np.arange(60).reshape((-1,1))+20\n",
    "y_new = model.predict(x_new)\n",
    "\n",
    "data.plot.scatter(x='Age',y='AbsentHours')\n",
    "plt.plot(x_new, y_new, color='r')\n",
    "plt.show()"
   ]
