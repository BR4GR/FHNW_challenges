   "source": [
    "# wir sehen Datenprobleme, da gewisse Absenzen mit 0 h eingetragen wurden.\n",
    "# hier finden wir sie mittels where\n",
    "index = np.where(data['AbsentHours'] == 0)\n",
    "print(index)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# hier werden unbrauchbare Zeilen entfernt\n",
    "data.drop(data.index[index],inplace=True, axis=0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
