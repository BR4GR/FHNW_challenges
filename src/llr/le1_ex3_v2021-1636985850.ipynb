   "source": [
    "#Residuen\n",
    "\n",
    "fig = plt.figure(figsize=(10,6))\n",
    "\n",
    "#Vorhersage durchfuehren und Residuen berechnen\n",
    "predictedabsence = model.predict(age)\n",
    "resid = absenthours-predictedabsence #Residuen\n",
    "\n",
    "plt.plot(age, resid, 'o',color='r')\n",
    "plt.show()"
   ]
